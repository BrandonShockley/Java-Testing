package com.bon.main;

import java.awt.GridBagConstraints;

public class ModdedGridBagConstraints extends GridBagConstraints {
	
	public ModdedGridBagConstraints ()
	{
		super();
	}
	
	public ModdedGridBagConstraints setAnchor (String field,int value)
	{
		switch(field)
		{
		case "anchor":
			this.anchor=value;
			break;
		case "weightx":
			this.weightx=value;
			break;
		case "weighty":
			this.weighty=value;
		case "":
		}
		return this;
	}

}
